{
  "family": "demo-app-green",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::456551560581:role/ecs-task-role",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "nginxdemos/hello:plain-text",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80
        }
      ]
    }
  ]
}
